{
  "name": "Create and Read QR Code",
  "description": "All QR codes one click away.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "default_icon": {
    "128": "/assets/icons/icon128.png"
  },
  "icons": {
    "128": "/assets/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
